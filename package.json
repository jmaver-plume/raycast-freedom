{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-freedom",
  "title": "Freedom",
  "description": "Freedom - Block Websites, Apps, and the Internet",
  "icon": "command-icon.png",
  "author": "jmaver",
  "license": "MIT",
  "commands": [
    {
      "name": "create-schedule",
      "title": "Create a schedule",
      "description": "Create a schedule",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.3",
    "axios": "^1.3.3",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  },
  "preferences": [
    {
      "name": "cookie",
      "title": "Cookie",
      "type": "password",
      "description": "Cookie header",
      "required": true
    },
    {
      "name": "xCsrfToken",
      "title": "X-CSRF-Token",
      "type": "password",
      "description": "X CSRF Token header",
      "required": true
    }
  ]
}
